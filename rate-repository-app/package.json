{
  "name": "rate-repository-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|react-router-native)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@expo/metro-config": "^0.19.12",
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-picker/picker": "^2.11.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "expo": "~52.0.46",
    "expo-linking": "^7.0.5",
    "expo-status-bar": "~2.0.1",
    "formik": "^2.4.6",
    "graphql": "^16.10.0",
    "lodash-es": "^4.17.21",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-web": "~0.19.13",
    "react-router-native": "^6.30.0",
    "use-debounce": "^10.0.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.6",
    "react-test-renderer": "^18.3.1"
  },
  "private": true
}
